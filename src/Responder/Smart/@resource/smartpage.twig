{% apply spaceless %}
	<!doctype html>
	<html lang="{{ smartpage.language }}">
	<head>
		{% with viewmodel %}
			{% block head %}
				<title>{{ smartpage.title }}</title>
				<meta charset="utf-8">
				<meta name="data" content="{{ smartpage.data }}">
				<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=.8, user-scalable=1">
				{% for css in smartpage.css %}
				<link rel="stylesheet" type="text/css" href="{{ css }}?v={{ smartpage.frontendVersion }}" />{% endfor %}
				{% for js in smartpage.js %}
				<script src="{{ js }}?v={{ smartpage.frontendVersion }}"></script>
				{% endfor %}
				<link rel="icon" href="data:;base64,iVBORw0KGgo=">
			{% endblock %}
		{% endwith %}
	</head>

	<body {% if smartpage.bodyclass %} class="{{ smartpage.class }}"{% endif %}>
	{% with viewmodel %}{% block content %}{% endblock %}{% endwith %}
	</body>

	</html>
{% endapply %}